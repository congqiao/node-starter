{
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --enable-source-maps dist",
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config src",
    "build": "tsc",
    "check:eslint": "eslint .",
    "fix:eslint": "eslint --fix .",
    "check:prettier": "prettier --check .",
    "fix:prettier": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
