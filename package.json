{
  "name": "node-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config src",
    "build": "webpack --define process.env.NODE_ENV=\"'production'\"",
    "lint": "eslint src",
    "format": "prettier --print-width 120 --no-semi --single-quote --trailing-comma all --write \"src/**/*.js\""
  },
  "devDependencies": {
    "dotenv": "*",
    "eslint": "*",
    "eslint-plugin-import": "*",
    "prettier": "*",
    "webpack": "*"
  }
}
